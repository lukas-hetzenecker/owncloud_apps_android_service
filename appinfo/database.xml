<?xml version="1.0" encoding="ISO-8859-1" ?>
<database>
 <name>*dbname*</name>
 <create>true</create>
 <overwrite>false</overwrite>
 <charset>utf8</charset>
 <table>
  <name>*dbprefix*android_services_login</name>
  <declaration>
    <field>
      <name>mail</name>
      <type>text</type>
      <notnull>true</notnull>
      <length>100</length>
    </field>
    <field>
      <name>passwd</name>
      <type>text</type>
      <notnull>true</notnull>
      <length>100</length>
    </field>
    <field>
      <name>auth_token</name>
      <type>text</type>
      <notnull>false</notnull>
      <length>100</length>
    </field>
    <field>
      <name>registration_id</name>
      <type>text</type>
      <notnull>false</notnull>
      <length>100</length>
    </field>
  </declaration>
</table>
</database>

